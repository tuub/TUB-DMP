{
    "name": "schalljugend/tub-dmp",
    "description": "TUB-DMP",
    "keywords": ["dmp", "research data", "management", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "5.5.*",
        "laravelcollective/html": "5.5.*",
        "iamcal/lib_autolink": "^1.7",
        "laracasts/flash": "^2.0",
        "felixkiss/uniquewith-validator": "^2.0",
        "baum/baum": "^1.1",
        "adamwathan/bootforms": "^0.8.4",
        "barryvdh/laravel-dompdf": "^0.8.0",
        "tck/odbc": "dev-master",
        "laravel/tinker": "^1.0",
        "razorbacks/laravel-shibboleth": "^2.2",
        "mrclay/shibalike": "^1.0",
        "webpatser/laravel-uuid": "^3.0",
        "davejamesmiller/laravel-breadcrumbs": "^4.2",
        "iatstuti/laravel-nullable-fields": "^1.5",
        "intervention/image": "^2.4"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "phpspec/phpspec": "~2.1",
        "rap2hpoutre/laravel-log-viewer": "^0.5.3",
        "doctrine/dbal": "~2.3",
        "barryvdh/laravel-debugbar": "^2.2",
        "laracasts/generators": "dev-master as 1.1.4",
        "themsaid/laravel-langman": "^1.1",
        "barryvdh/laravel-ide-helper": "^2.4",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/AppHelper.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan optimize"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
