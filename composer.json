{
    "name": "schalljugend/tub-dmp",
    "description": "TUB-DMP",
    "keywords": ["dmp", "research data", "management", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "laravelcollective/html": "5.3.*",
        "iamcal/lib_autolink": "^1.7",
        "laracasts/flash": "^2.0",
        "felixkiss/uniquewith-validator": "^2.0",
        "baum/baum": "^1.1",
        "adamwathan/bootforms": "^0.8.4",
        "barryvdh/laravel-snappy": "^0.3.3",
        "h4cc/wkhtmltopdf-amd64": "dev-master",
        "barryvdh/laravel-dompdf": "^0.8.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "phpspec/phpspec": "~2.1",
        "rap2hpoutre/laravel-log-viewer": "^0.5.3",
        "doctrine/dbal": "~2.3",
        "barryvdh/laravel-debugbar": "^2.2",
        "laracasts/generators": "^1.1",
        "themsaid/laravel-langman": "^1.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/exporters.php",
            "app/Helpers/AppHelper.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
